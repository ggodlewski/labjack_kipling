{
	"logging_config": {
		"file_prefix": "basic_config",
		"write_to_file": false,
		"default_result_view": "0",
		"default_result_file": "0"
	},


	"view_config": {
		"update_rate_ms": 100
	},
	"views": [
		"basic_view",
		"current_values_view"
	],
	"basic_view": {
		"name": "Basic Graph",
		"view_type": "basic_graph",
		"window_size": 100,
		"group": "basic_data_group"
	},
	"current_values_view": {
		"name": "Current Values",
		"view_type": "current_values",
		"group": "basic_data_group"
	},


	"data_groups": [
		"basic_data_group",
		"secondary_data_group"
	],
	"basic_data_group": {
		"group_period_ms": 100,
		"group_name": "basic_data_group",

		"device_serial_numbers": ["1", "2"],
		"1": {
			"registers": [{
				"name": "AIN0",
				"id": "1-AIN0",
				"format": "default"
			}]
		},
		"2": {
			"registers": [{
				"name": "AIN0",
				"id": "2-AIN0",
				"format": "custom_func",
				"format_func": "console.log('Custom Func', val); return val;"
			}]
		}
	},
	"secondary_data_group": {
		"group_period_ms": 50,
		"group_name": "secondary_data_group",
		"device_serial_numbers": ["1"],
		"1": {
			"registers": [{
				"name": "AIN1",
				"id": "1-AIN1",
				"format": "default"
			}]
		}
	}
}